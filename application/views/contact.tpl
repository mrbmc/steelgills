	<h2>Contact Form</h2>

	<form action="/contact/send" method="post" id="contact_form">	
	<fieldset>
		<div>
			<label>Name</label>
			<span><input type="text" name="name" id="name" class="text" /></span>
			<span class="status"></span>
		</div>
		<div>
			<label for="email">Email</label>
			<span><input type="text" name="email" id="email" class="text" /></span>
			<span class="status"></span>
		</div>
		<div> 
			<label for="subject">Topic *</label><br /> 
			<select name="subject" id="subject" class="formText tan short"> 
				<option value="Email from Feeback Form: I have a question" label="I have a question">I have a question</option> 
				<option value="Email from Feeback Form: Advertising Inquiry" label="Advertising inquiry">Advertising inquiry</option> 
				<option value="Email from Feeback Form: Partnership Inquiry" label="Partnership inquiry">Partnership inquiry</option> 
				<option value="Email from Feeback Form: Site Feedback" label="Site Feedback">Site Feedback</option> 
				<option value="Email from Feeback Form: Bug" label="Report a bug">Report a bug</option> 
			</select> 
		</div>
		<div>
			<label for="message">Message *</label><br /> 
			<textarea name="message" id="message" class="txtInput" rows="5" cols="40"></textarea> 
		</div> 

		<div>
			<label></label>
			<span><small class="highlight"><b>*</b> required field</small></span>
		</div>

		<div>
			<label></label>
			<span><input type="submit" class="button" id="submit" name="Submit" value="Send my Message" /></span>
		</div>

	</fieldset>

	</form> 
	
